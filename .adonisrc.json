{
  "typescript": true,
  "commands": [
    "./infrastructure/framework/commands",
    "@adonisjs/core/build/commands",
    "@adonisjs/repl/build/commands",
    "@adonisjs/lucid/build/commands"
  ],
  "directories": {
    "config": "config",
    "public": "public",
    "contracts": "infrastructure/framework/contracts",
    "providers": "providers",
    "database": "infrastructure/database",
    "migrations": "infrastructure/database/migrations",
    "seeds": "infrastructure/database/seeders",
    "resources": "resources",
    "views": "resources/views",
    "start": "start",
    "tmp": "tmp",
    "tests": "tests"
  },
  "namespaces": {
    "models": "App/Models",
    "middleware": "App/Middleware",
    "exceptions": "App/Exceptions",
    "validators": "App/Validators",
    "httpControllers": "App/Controllers",
    "eventListeners": "App/Listeners",
    "redisListeners": "App/Listeners"
  },
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "app",
    "Config": "config",
    "Database": "database",
    "Contracts": "contracts"
  },
  "preloads": ["./start/routes", "./start/kernel"],
  "providers": [
    "./infrastructure/framework/providers/AppProvider",
    "@adonisjs/core",
    "@adonisjs/lucid",
    "@adonisjs/auth"
  ],
  "aceProviders": ["@adonisjs/repl"]
}
